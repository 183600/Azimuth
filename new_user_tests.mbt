test "financial_calculation_scenario" {
  // æµ‹è¯•é‡‘èè®¡ç®—åœºæ™¯
  let principal = 10000  // æœ¬é‡‘
  let rate = 5          // åˆ©ç‡ï¼ˆç™¾åˆ†æ¯”ï¼‰
  let years = 3         // å¹´æ•°
  
  // è®¡ç®—ç®€å•åˆ©æ¯
