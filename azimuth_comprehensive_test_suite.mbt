test "complex_calculations" {
  // æµ‹è¯•å¤æ‚è®¡ç®—åœºæ™¯
  let a = 10
  let b = 5
  let c = 2
  
  // (a + b) * c
  let result1 = multiply(add(a, b), c)
  assert_eq(30, result1)
  
  // a * b + a * c
  let result2 = add(multiply(a, b), multiply(a, c))
  assert_eq(70, result2)
  
  // (a - b) * (a + b)
  let result3 = multiply(add(a, -b), add(a, b))
  assert_eq(75, result3)
}

