test "real_world_application" {
  // æµ‹è¯•å®é™…åº”ç”¨åœºæ™¯ï¼šè®¡ç®—è´­ç‰©è½¦æ€»ä»·
  let item_price = 199
  let item_quantity = 3
  let shipping_fee = 15
  let tax_rate = 8  // 8%
  
  // è®¡ç®—å•†å“æ€»ä»·
  let subtotal = multiply(item_price, item_quantity)
  
  // è®¡ç®—ç¨è´¹
