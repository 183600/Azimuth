test "complex_nested_operations" {
  // æµ‹è¯•å¤æ‚åµŒå¥—æ“ä½œ
  let a = 5
  let b = 3
  let c = 2
  
  // è®¡ç®— (a + b) * (c + 1)
  let sum1 = add(a, b)
  let sum2 = add(c, 1)
  let result = multiply(sum1, sum2)
  assert_eq(40, result)
  
  // è®¡ç®— a * b + c * (a + b)
  let product1 = multiply(a, b)
  let sum3 = add(a, b)
  let product2 = multiply(c, sum3)
  let result2 = add(product1, product2)
  assert_eq(31, result2)
}

