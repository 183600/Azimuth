}

test "real_world_shopping_cart" {
  // æµ‹è¯•çœŸå®ä¸–ç•Œè´­ç‰©è½¦åœºæ™¯
  let item1_price = 299
  let item1_quantity = 2
  let item2_price = 199
  let item2_quantity = 3
  let discount = 100
  
  // è®¡ç®—æ¯ç§å•†å“æ€»ä»·
  let total1 = multiply(item1_price, item1_quantity)
  let total2 = multiply(item2_price, item2_quantity)
  
  // è®¡ç®—è´­ç‰©è½¦æ€»ä»·
  let subtotal = add(total1, total2)
  let final_total = add(subtotal, -discount)
  
  // éªŒè¯æœ€ç»ˆä»·æ ¼ï¼š(299*2 + 199*3) - 100 = (598 + 597) - 100 = 1195 - 100 = 1095
  assert_eq(1095, final_total)
}

test "temperature_conversion" {
  // æµ‹è¯•æ¸©åº¦è½¬æ¢åœºæ™¯
  let celsius = 25
  let fahrenheit = 77
  
  // æ¨¡æ‹Ÿæ‘„æ°åº¦è½¬åæ°åº¦ï¼šF = C * 9/5 + 32
  let c_to_f_result = add(multiply(celsius, 9), 160) / 5
  assert_eq(fahrenheit, c_to_f_result)
  
  // æ¨¡æ‹Ÿåæ°åº¦è½¬æ‘„æ°åº¦ï¼šC = (F - 32) * 5/9
  let f_to_c_result = multiply(add(fahrenheit, -32), 5) / 9
  assert_eq(celsius, f_to_c_result)
}