test "complex_calculation_scenarios" {
  // å¤æ‚è®¡ç®—åœºæ™¯æµ‹è¯•
  // åœ†çš„å‘¨é•¿å’Œé¢ç§¯è®¡ç®—ï¼ˆç®€åŒ–ç‰ˆï¼‰
  let radius = 7
  let pi_approx = 3
  let circumference = multiply(multiply(2, pi_approx), radius)
  let area = multiply(pi_approx, multiply(radius, radius))
  assert_eq(42, circumference)  // 2 Ã— 3 Ã— 7 = 42
  assert_eq(147, area)          // 3 Ã— 7 Ã— 7 = 147
  
  // æ¸©åº¦è½¬æ¢ï¼šæ‘„æ°åº¦è½¬åæ°åº¦ï¼ˆç®€åŒ–ç‰ˆï¼šF = C Ã— 2 + 30ï¼‰
  let celsius = 18
  let fahrenheit = add(multiply(celsius, 2), 30)
  assert_eq(66, fahrenheit)  // 18 Ã— 2 + 30 = 66
  
  // ç®€å•åˆ©æ¯è®¡ç®—ï¼šæœ¬é‡‘ Ã— (1 + åˆ©ç‡ Ã— æ—¶é—´)
  let principal = 5000
  let rate = 8
  let time = 3
  let total_amount = multiply(principal, add(1, multiply(rate, time)))
  assert_eq(125000, total_amount)  // 5000 Ã— (1 + 8Ã—3) = 5000 Ã— 25 = 125000
}

