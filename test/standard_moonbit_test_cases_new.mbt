test "complex_business_calculation" {
  // å¤æ‚ä¸šåŠ¡è®¡ç®—åœºæ™¯ï¼šé¡¹ç›®èµ„æºè§„åˆ’
  let team_size = 8
  let tasks_per_person = 5
