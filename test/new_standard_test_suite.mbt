test "financial_compound_interest" {
  // é‡‘èå¤åˆ©è®¡ç®—ï¼ˆç®€åŒ–ç‰ˆï¼‰
  let principal = 1000
  let interest_rate = 5
  
  let year1 = add(principal, divide_with_ceil(multiply(principal, interest_rate), 100))
