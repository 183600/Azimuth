test "performance_calculations" {
  // æ€§èƒ½ç›¸å…³è®¡ç®—æµ‹è¯•
  // è®¡ç®—ç­‰å·®æ•°åˆ—å’Œï¼š1 + 2 + ... + n = n Ã— (n + 1) / 2
  let n = 100
  let series_sum = azimuth.multiply(n, azimuth.add(n, 1)) / 2
  @assertion.assert_eq(5050, series_sum)?
  
  // è®¡ç®—ç­‰æ¯”æ•°åˆ—å’Œï¼š2^0 + 2^1 + ... + 2^6 = 2^7 - 1 = 127
