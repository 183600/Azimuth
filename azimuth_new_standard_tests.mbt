  let mul_underflow_result = azimuth.multiply(min_val, min_val)
  @assertion.assert_eq(max_val, mul_underflow_result)?  // è´Ÿè´Ÿå¾—æ­£ï¼Œä½†ä¼šæº¢å‡º
}