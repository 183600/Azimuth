  let final_total = add(subtotal, shipping)
  
  assert_eq(717, final_total)
  
  // åœºæ™¯2ï¼šæ¸©åº¦è½¬æ¢ï¼ˆæ‘„æ°åº¦è½¬åæ°åº¦ï¼Œç®€åŒ–ç‰ˆï¼šF = C Ã— 2 + 30ï¼‰
  let celsius = 25
