  assert_eq(80, fahrenheit)
  
  // åœºæ™¯3ï¼šè®¡ç®—å¹³å‡åˆ†
  let score1 = 85
  let score2 = 90
  let score3 = 95
  let average = add(add(score1, score2), score3) / 3
  assert_eq(90, average)
}

