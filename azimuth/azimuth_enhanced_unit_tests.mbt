  let total_tasks = 127
  let team_members = 5
  let tasks_per_person_per_day = 3
  let work_days_per_week = 5
  
  // è®¡ç®—æ¯æ—¥å¤„ç†èƒ½åŠ›
  let daily_capacity = multiply(team_members, tasks_per_person_per_day)
  
  // è®¡ç®—éœ€è¦çš„å·¥ä½œæ—¥
  let work_days_needed = divide_with_ceil(total_tasks, daily_capacity)
  
  // è®¡ç®—éœ€è¦çš„å‘¨æ•°
