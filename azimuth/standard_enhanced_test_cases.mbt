  let product_a = 35  // äº§å“Aæ•°é‡
  let product_b = 42  // äº§å“Bæ•°é‡
  let box_capacity = 20
  
  let boxes_a = divide_with_ceil(product_a, box_capacity)  // ceil(35/20) = 2
