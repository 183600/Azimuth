test "complex_calculation" {
  // æµ‹è¯•å¤åˆè®¡ç®—ï¼š(a+b)*(c-d)
  let a = 10
  let b = 20
  let c = 30
  let d = 15
  let sum = @azimuth.add(a, b)
  let diff = @azimuth.add(c, -d)
  let result = @azimuth.multiply(sum, diff)
  @azimuth.assert_eq(150, result) // (10+20)*(30-15) = 30*15 = 450
}

