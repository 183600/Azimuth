// 测试辅助函数定义
fn add(a : Int, b : Int) -> Int {
  a + b
}

fn multiply(a : Int, b : Int) -> Int {
  a * b
}

fn greet(name : String) -> String {
  "Hello, " + name + "!"
}

fn assert_eq(expected : Int, actual : Int) -> Unit {
  if expected != actual {
    @builtin.panic()
  }
}

fn assert_eq_string(expected : String, actual : String) -> Unit {
  if expected != actual {
    @builtin.panic()
  }
}

fn assert_true(condition : Bool) -> Unit {
  if !condition {
    @builtin.panic()
  }
}

fn assert_false(condition : Bool) -> Unit {
  if condition {
    @builtin.panic()
  }
}

test "test_2_edge_cases_add_additional" {
  // Test with zero
  assert_eq(5, add(0, 5))
  assert_eq(5, add(5, 0))
  // Test with large numbers
  assert_eq(2147483647, add(2147483646, 1))
  assert_eq(-2147483648, add(-2147483647, -1))
}
test "test_2_edge_cases_multiply_additional" {
  // Test with zero
  assert_eq(0, multiply(0, 5))
  assert_eq(0, multiply(5, 0))
  // Test with one
  assert_eq(5, multiply(1, 5))
  assert_eq(5, multiply(5, 1))
  // Test with negative one
  assert_eq(-5, multiply(-1, 5))
  assert_eq(-5, multiply(5, -1))
  assert_eq(5, multiply(-1, -5))
}
test "test_2_edge_cases_greet_additional" {
  // Test with special characters
  assert_eq_string("Hello, @#$!", greet("@#$"))
  assert_eq_string("Hello, 123!", greet("123"))
}
