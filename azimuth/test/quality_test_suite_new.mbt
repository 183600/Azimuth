  // æå€¼ç»„åˆè¿ç®—
  let extreme_product = @azimuth.multiply(1000, 1000)
  let extreme_sum = @azimuth.add(extreme_product, max_val - 1000000)
  @azimuth.assert_eq(max_val, extreme_sum)
}

