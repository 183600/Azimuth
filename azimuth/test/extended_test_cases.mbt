test "algorithm_complexity_tests" {
  // æµ‹è¯•ç®—æ³•å¤æ‚åº¦ç›¸å…³çš„æµ‹è¯•
  
  // æµ‹è¯•æ–æ³¢é‚£å¥‘æ•°åˆ—è®¡ç®—ï¼ˆç®€åŒ–ç‰ˆï¼‰
  let fib_n = 10
  let mut a = 0
  let mut b = 1
  let mut i = 2
  while i <= fib_n {
    let temp = @azimuth.add(a, b)
    a = b
    b = temp
    i = @azimuth.add(i, 1)
  }
  // ç¬¬10ä¸ªæ–æ³¢é‚£å¥‘æ•°æ˜¯55
  @azimuth.assert_eq(55, b)
  
  // æµ‹è¯•å¹‚è¿ç®—ï¼ˆä½¿ç”¨ä¹˜æ³•å®ç°ï¼‰
  let base = 3
  let exponent = 5
  let mut result = 1
  let mut j = 1
  while j <= exponent {
    result = @azimuth.multiply(result, base)
    j = @azimuth.add(j, 1)
  }
  // 3^5 = 243
  @azimuth.assert_eq(243, result)
  
  // æµ‹è¯•æœ€å¤§å…¬çº¦æ•°ç®—æ³•ï¼ˆç®€åŒ–ç‰ˆï¼Œæ¬§å‡ é‡Œå¾—ç®—æ³•ï¼‰
  let m = 48
  let n = 18
  let mut a = m
  let mut b = n
  while b != 0 {
    let temp = b
    b = a % b  // ä½¿ç”¨å–æ¨¡è¿ç®—
    a = temp
  }
  // 48å’Œ18çš„æœ€å¤§å…¬çº¦æ•°æ˜¯6
  @azimuth.assert_eq(6, a)
}