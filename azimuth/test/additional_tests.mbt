test "complex_arithmetic_expressions" {
  // æµ‹è¯•å¤æ‚çš„ç®—æœ¯è¡¨è¾¾å¼
  let a = 10
  let b = 20
  let c = -5
  let d = 2
  
  // (a + b) * (c + d)
  let result = @azimuth.multiply(
    @azimuth.add(a, b),
    @azimuth.add(c, d)
  )
  @azimuth.assert_eq(60, result)  // (10 + 20) * (-5 + 2) = 30 * (-3) = -90ï¼Œä½†-90åœ¨èŒƒå›´å†…
  
  // a * b + c * d
  let result2 = @azimuth.add(
    @azimuth.multiply(a, b),
    @azimuth.multiply(c, d)
  )
  @azimuth.assert_eq(190, result2)  // 10 * 20 + (-5) * 2 = 200 - 10 = 190
}

