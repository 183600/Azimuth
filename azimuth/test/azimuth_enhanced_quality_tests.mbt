// æµ‹è¯•4: æ•°å­¦æ’ç­‰å¼éªŒè¯
test "mathematical_identities_verification" {
  let x = 42
  let y = 17
  let z = 8
  
  // åŠ æ³•æ’ç­‰å¼
  @azimuth.assert_eq(x, @azimuth.add(x, 0))
  @azimuth.assert_eq(@azimuth.add(x, y), @azimuth.add(y, x))  // äº¤æ¢å¾‹
  @azimuth.assert_eq(@azimuth.add(@azimuth.add(x, y), z), @azimuth.add(x, @azimuth.add(y, z)))  // ç»“åˆå¾‹
  
  // ä¹˜æ³•æ’ç­‰å¼
  @azimuth.assert_eq(x, @azimuth.multiply(x, 1))
  @azimuth.assert_eq(0, @azimuth.multiply(x, 0))
  @azimuth.assert_eq(@azimuth.multiply(x, y), @azimuth.multiply(y, x))  // äº¤æ¢å¾‹
  
  // åˆ†é…å¾‹
  @azimuth.assert_eq(
    @azimuth.multiply(x, @azimuth.add(y, z)),
    @azimuth.add(@azimuth.multiply(x, y), @azimuth.multiply(x, z))
  )
}

