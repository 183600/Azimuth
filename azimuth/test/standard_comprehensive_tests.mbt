  @azimuth.assert_eq(225, fahrenheit)  // 25 * 9 = 225
  
  // åœºæ™¯3: è®¡ç®—ç®€å•åˆ©æ¯
  let principal = 10000
  let rate = 5
  let time = 3
  let interest = @azimuth.multiply(principal, @azimuth.multiply(rate, time))
  @azimuth.assert_eq(150000, interest)  // 10000 * 5 * 3
}

