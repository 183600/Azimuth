    // é™¤æ³•ä¸€è‡´æ€§æµ‹è¯•ï¼ˆé™¤æ•°ä¸ä¸º0ï¼‰
    if (val != 0) {
      assert_eq(1, divide_with_ceil(val, val))
      assert_eq(val, divide_with_ceil(multiply(val, 2), 2))
    }
  }
  
  // å­—ç¬¦ä¸²ä¸€è‡´æ€§æµ‹è¯•
  let test_strings = ["", "a", "test", "æµ‹è¯•", "ğŸ‰", "test@example.com"]
  
