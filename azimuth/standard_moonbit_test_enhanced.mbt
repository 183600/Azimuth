test "business_logistics_calculation" {
  // ä¸šåŠ¡ç‰©æµè®¡ç®—æµ‹è¯•
  // åœºæ™¯ï¼šè®¡ç®—ä»“åº“å­˜å‚¨éœ€æ±‚
  let products = 158
  let boxes_per_pallet = 24
  let pallets_needed = divide_with_ceil(products, boxes_per_pallet)
  
  assert_eq(7, pallets_needed)
  
  // éªŒè¯æ€»å­˜å‚¨å®¹é‡
