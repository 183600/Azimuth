test "sequential_operations" {
  // æµ‹è¯•è¿ç»­æ“ä½œ
  let initial = 8
  let step1 = @azimuth.add(initial, 4)        // 12
  let step2 = @azimuth.multiply(step1, 3)     // 36
  let step3 = @azimuth.divide_with_ceil(step2, 10)  // 4
  let step4 = @azimuth.add(step3, 2)          // 6
  
  @azimuth.assert_eq(6, step4)
}