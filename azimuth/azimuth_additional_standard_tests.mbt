  let total_products = 157
  let box_capacity = 24
  let boxes_needed = @azimuth.divide_with_ceil(total_products, box_capacity)
  
  // éªŒè¯éœ€è¦çš„ç®±å­æ•°é‡
  @azimuth.assert_eq(7, boxes_needed)
  
  // éªŒè¯æ€»å®¹é‡
