    Counter::add(counter, 1.0)
    Histogram::record(histogram, i.to_double())
  }
  
  let meter_end = Clock::now_unix_nanos(Clock::system())
  let meter_duration = meter_end - meter_start
  
  assert_true(meter_duration < 5000000000L)  // å°äº5ç§’
}

