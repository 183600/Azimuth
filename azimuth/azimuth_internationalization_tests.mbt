  let russian_key = azimuth::ContextKey::new("Ñ€ÑƒÑÑĞºĞ¸Ğ¹ ĞºĞ»ÑÑ‡")
  let ctx_with_russian = azimuth::Context::with_value(ctx_with_arabic, russian_key, "Ñ€ÑƒÑÑĞºĞ¾Ğµ Ğ·Ğ½Ğ°Ñ‡ĞµĞ½Ğ¸Ğµ")
  
  let emoji_key = azimuth::ContextKey::new("emoji-key-ğŸš€")
  let ctx_with_emoji = azimuth::Context::with_value(ctx_with_russian, emoji_key, "emoji-value-ğŸ‰")
  
  // Create baggage with internationalized entries
  let baggage = azimuth::Baggage::new()
  let baggage_with_international = azimuth::Baggage::set_entry(baggage, "å›½é™…åŒ–æµ‹è¯•", "internationalization-test")
  let baggage_with_japanese = azimuth::Baggage::set_entry(baggage_with_international, "æ—¥æœ¬èªãƒ†ã‚¹ãƒˆ", "japanese-test")
  let baggage_with_korean = azimuth::Baggage::set_entry(baggage_with_japanese, "í•œêµ­ì–´ í…ŒìŠ¤íŠ¸", "korean-test")
  let baggage_with_arabic = azimuth::Baggage::set_entry(baggage_with_korean, "Ø§Ø®ØªØ¨Ø§Ø± Ø¹Ø±Ø¨ÙŠ", "arabic-test")
  let baggage_with_russian = azimuth::Baggage::set_entry(baggage_with_arabic, "Ñ€ÑƒÑÑĞºĞ¸Ğ¹ Ñ‚ĞµÑÑ‚", "russian-test")
