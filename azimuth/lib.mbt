  // è®¡ç®—æ€»ä½“æ€§èƒ½å¾—åˆ†
  let performance_score = add(add(add_efficiency, multiply_efficiency), divide_efficiency)
  assert_eq(24, performance_score)  // 10 + 10 + 4
}

