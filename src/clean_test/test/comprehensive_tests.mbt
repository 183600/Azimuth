///|
test "add_commutative_property" {
  // æµ‹è¯•åŠ æ³•äº¤æ¢å¾‹ a + b = b + a
  assert_eq(@clean_test.add(123, 456), @clean_test.add(456, 123))
  assert_eq(@clean_test.add(-123, 456), @clean_test.add(456, -123))
  assert_eq(@clean_test.add(-123, -456), @clean_test.add(-456, -123))
  assert_eq(@clean_test.add(0, 123), @clean_test.add(123, 0))
