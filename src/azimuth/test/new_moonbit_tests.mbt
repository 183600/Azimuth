test "conditional_logic_scenarios" {
  // æµ‹è¯•æ¡ä»¶é€»è¾‘åœºæ™¯
  
  // åŸºäºæ¯”è¾ƒç»“æœçš„æ¡ä»¶è¿ç®—
  let x = 25
  let y = 30
  let z = 15
  
  // å¦‚æœ x < yï¼Œåˆ™è®¡ç®— x + zï¼Œå¦åˆ™è®¡ç®— x * z
  let condition1 = x < y
  let result1 = if condition1 { azimuth::@azimuth.add(x, z) } else { azimuth::@azimuth.multiply(x, z) }
  @azimuth.assert_eq(40, result1)  // 25 < 30 ä¸ºçœŸï¼Œæ‰€ä»¥ 25 + 15 = 40
  
  // å¦‚æœ y > zï¼Œåˆ™è®¡ç®— y * zï¼Œå¦åˆ™è®¡ç®— y + z
  let condition2 = y > z
  let result2 = if condition2 { azimuth::@azimuth.multiply(y, z) } else { azimuth::@azimuth.add(y, z) }
  @azimuth.assert_eq(450, result2)  // 30 > 15 ä¸ºçœŸï¼Œæ‰€ä»¥ 30 * 15 = 450
  
  // å¤æ‚æ¡ä»¶
  let complex_condition = azimuth::@azimuth.add(x, y) > azimuth::@azimuth.multiply(z, 3)
  let complex_result = if complex_condition { azimuth::@azimuth.multiply(x, y) } else { azimuth::@azimuth.add(x, y) }
  @azimuth.assert_eq(750, complex_result)  // 25 + 30 > 15*3 (55 > 45) ä¸ºçœŸï¼Œæ‰€ä»¥ 25 * 30 = 750
}
