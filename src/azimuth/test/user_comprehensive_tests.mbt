test "complex_business_logic" {
  // æµ‹è¯•å¤æ‚ä¸šåŠ¡é€»è¾‘ï¼šè®¡ç®—è´·æ¬¾è¿˜æ¬¾
  let principal = 100000  // æœ¬é‡‘
  let annual_rate = 6     // å¹´åˆ©ç‡ï¼ˆç™¾åˆ†æ¯”ï¼‰
  let years = 10          // è´·æ¬¾å¹´é™
  
  // ç®€åŒ–çš„åˆ©æ¯è®¡ç®—ï¼šæ€»åˆ©æ¯ = æœ¬é‡‘ Ã— å¹´åˆ©ç‡ Ã— å¹´æ•°
  let total_interest = @azimuth.multiply(@azimuth.multiply(principal, annual_rate), years)
  
  // æ€»è¿˜æ¬¾é¢ = æœ¬é‡‘ + æ€»åˆ©æ¯
  let total_payment = @azimuth.add(principal, total_interest)
  
  // å¹´è¿˜æ¬¾é¢ = æ€»è¿˜æ¬¾é¢ / å¹´æ•°
  let annual_payment = total_payment / years
  
  // éªŒè¯ç»“æœï¼š100000 + 100000*6*10 = 100000 + 6000000 = 6100000
  // å¹´è¿˜æ¬¾é¢ï¼š6100000 / 10 = 610000
  @azimuth.assert_eq(6100000, total_payment)
