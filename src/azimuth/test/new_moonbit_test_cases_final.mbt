  let unit_price = 199
  let quantity = 5
  let discount_rate = 10
  let tax_rate = 8
  
  // è®¡ç®—æ€»ä»·
  let subtotal = @azimuth.multiply(unit_price, quantity)
  // è®¡ç®—æŠ˜æ‰£
