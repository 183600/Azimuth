test "complex_calculation_sequence" {
  // æµ‹è¯•å¤æ‚è®¡ç®—åºåˆ—
  let a = 10
  let b = 20
  let c = 30
  
  // è®¡ç®— (a + b) * c - (a * b)
  let sum = @azimuth.add(a, b)
  let product1 = @azimuth.multiply(sum, c)
  let product2 = @azimuth.multiply(a, b)
  let result = @azimuth.add(product1, -product2)
  
