///|
test "multiply_power_of_two_patterns" {
  // æµ‹è¯•2çš„å¹‚æ¬¡æ–¹æ¨¡å¼
  assert_eq(2, @azimuth.multiply(1, 2))
  assert_eq(4, @azimuth.multiply(2, 2))
  assert_eq(8, @azimuth.multiply(4, 2))
  assert_eq(16, @azimuth.multiply(8, 2))
