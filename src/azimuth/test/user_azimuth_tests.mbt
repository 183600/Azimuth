}

test "divide_with_ceil_edge_cases" {
  // æµ‹è¯•å‘ä¸Šå–æ•´é™¤æ³•çš„è¾¹ç•Œæƒ…å†µ
  // åŸºæœ¬åŠŸèƒ½æµ‹è¯•
  assert_eq(4, divide_with_ceil(10, 3))  // 3.333... -> 4
