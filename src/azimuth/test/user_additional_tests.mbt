  let final_total = @azimuth.add(subtotal, shipping)
  
  // éªŒè¯ç»“æœï¼š199*2 + 299*1 + 99*3 + 20 = 398 + 299 + 297 + 20 = 1014
  @azimuth.assert_eq(1014, final_total)
}

