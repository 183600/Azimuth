  let item1_price = 199
  let item1_qty = 2
  let item2_price = 299
  let item2_qty = 1
  let item3_price = 99
  let item3_qty = 3
  let shipping = 20
  
  // è®¡ç®—å„é¡¹æ€»ä»·
  let total1 = @azimuth.multiply(item1_price, item1_qty)
