test "compound_calculations" {
  // æµ‹è¯•å¤åˆè®¡ç®—åœºæ™¯
  // è®¡ç®— (a + b) * (c - d) + e
  let a = 10
  let b = 5
  let c = 20
  let d = 8
  let e = 15
  
  let sum_ab = @azimuth.add(a, b)  // 15
  let diff_cd = @azimuth.add(c, -d)  // 12
  let product = @azimuth.multiply(sum_ab, diff_cd)  // 180
  let result = @azimuth.add(product, e)  // 195
  
  @azimuth.assert_eq(195, result)
}

