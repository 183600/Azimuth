  let principal = 10000  // æœ¬é‡‘
  let rate = 5          // åˆ©ç‡ï¼ˆç™¾åˆ†æ¯”ï¼‰
  let years = 3         // å¹´æ•°
  
  // ç®€å•åˆ©æ¯è®¡ç®—ï¼šåˆ©æ¯ = æœ¬é‡‘ Ã— åˆ©ç‡ Ã— å¹´æ•°
  let interest = @azimuth.multiply(principal, @azimuth.multiply(rate, years))
  
  // æœ¬æ¯å’Œ = æœ¬é‡‘ + åˆ©æ¯
  let total = @azimuth.add(principal, interest)
  
