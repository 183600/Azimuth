}

test "complex_calculations" {
  // æµ‹è¯•å¤åˆè¿ç®—ï¼šå¤šä¸ªå‡½æ•°çš„ç»„åˆä½¿ç”¨
  let length = 12
  let width = 8
  let height = 5
  
  // è®¡ç®—çŸ©å½¢é¢ç§¯ï¼šé•¿ Ã— å®½
  let area = @azimuth.multiply(length, width)
  
  // è®¡ç®—ä½“ç§¯ï¼šé¢ç§¯ Ã— é«˜
  let volume = @azimuth.multiply(area, height)
  
  // è®¡ç®—è¡¨é¢ç§¯ï¼ˆç®€åŒ–ï¼‰ï¼š2 Ã— (é•¿Ã—å®½ + é•¿Ã—é«˜ + å®½Ã—é«˜)
  let side1 = @azimuth.multiply(length, width)
  let side2 = @azimuth.multiply(length, height)
  let side3 = @azimuth.multiply(width, height)
  let surface_area = @azimuth.multiply(2, @azimuth.add(@azimuth.add(side1, side2), side3))
  
  @azimuth.assert_eq(96, area)      // 12 Ã— 8 = 96
  @azimuth.assert_eq(480, volume)   // 96 Ã— 5 = 480
  @azimuth.assert_eq(392, surface_area)  // 2 Ã— (96 + 60 + 40) = 392
}

