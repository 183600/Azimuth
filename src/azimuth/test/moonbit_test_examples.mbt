test "complex_calculation_scenario" {
  // æµ‹è¯•å¤æ‚è®¡ç®—åœºæ™¯ï¼šè®¡ç®—è´­ç‰©è½¦æ€»ä»·
  let item1_price = 299
  let item1_quantity = 2
  let item2_price = 199
  let item2_quantity = 3
  let shipping_fee = 15
  let discount = 50
  
  // è®¡ç®—æ¯ç§å•†å“çš„æ€»ä»·
  let total1 = @azimuth.multiply(item1_price, item1_quantity)
  let total2 = @azimuth.multiply(item2_price, item2_quantity)
  
  // è®¡ç®—å•†å“æ€»ä»·
  let subtotal = @azimuth.add(total1, total2)
  
  // åº”ç”¨æŠ˜æ‰£å’Œè¿è´¹
  let final_total = @azimuth.add(@azimuth.add(subtotal, -discount), shipping_fee)
  
  // éªŒè¯ç»“æœï¼š299*2 + 199*3 - 50 + 15 = 598 + 597 - 50 + 15 = 1160
  @azimuth.assert_eq(1160, final_total)
}

test "mathematical_properties_verification" {
  // æµ‹è¯•æ•°å­¦æ€§è´¨éªŒè¯
  let a = 15
  let b = 8
  let c = 7
  
  // åŠ æ³•äº¤æ¢å¾‹ï¼ša + b = b + a
  @azimuth.assert_eq(@azimuth.add(a, b), @azimuth.add(b, a))
  
  // ä¹˜æ³•äº¤æ¢å¾‹ï¼ša * b = b * a
  @azimuth.assert_eq(@azimuth.multiply(a, b), @azimuth.multiply(b, a))
  
  // åˆ†é…å¾‹ï¼ša * (b + c) = a * b + a * c
  @azimuth.assert_eq(
    @azimuth.multiply(a, @azimuth.add(b, c)),
    @azimuth.add(@azimuth.multiply(a, b), @azimuth.multiply(a, c))
  )
  
  // ç»“åˆå¾‹ï¼š(a + b) + c = a + (b + c)
  @azimuth.assert_eq(
    @azimuth.add(@azimuth.add(a, b), c),
    @azimuth.add(a, @azimuth.add(b, c))
  )
}

