  let remaining_budget = @azimuth.add(total_budget, -allocated_budget)
  
  // è®¡ç®—å¯ä»¥æ”¯æŒçš„é¢å¤–å°é¡¹ç›®æ•°é‡ï¼ˆæ¯ä¸ªé¡¹ç›®5000ï¼‰
  let additional_projects = @azimuth.divide_with_ceil(remaining_budget, 5000)
  
