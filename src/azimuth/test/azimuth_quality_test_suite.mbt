  let apple_price = 8
  let apple_qty = 3
  let banana_price = 5
  let banana_qty = 4
  let shipping_fee = 12
  
  // è®¡ç®—å•†å“æ€»ä»·
  let apple_total = @azimuth.multiply(apple_price, apple_qty)
