  let a = 123
  let b = 456
  
  // åŠ æ³•äº¤æ¢å¾‹: a + b = b + a
  assert_eq(azimuth::add(a, b), azimuth::add(b, a))
  
  // ä¹˜æ³•äº¤æ¢å¾‹: a * b = b * a
