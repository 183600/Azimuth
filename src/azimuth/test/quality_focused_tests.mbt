  // éªŒè¯æ€»å®¹é‡æ˜¯å¦è¶³å¤Ÿ
  let total_capacity = @azimuth.multiply(boxes_needed, items_per_box)
  let difference = total_capacity - total_items
  @azimuth.assert_eq(true, difference >= 0)
}

