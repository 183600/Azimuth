  azimuth::assert_eq(697, cart_total)  // 199*2 + 299*1 = 398 + 299 = 697
  
  // æ¸©åº¦è½¬æ¢ï¼ˆç®€åŒ–ç‰ˆï¼‰ï¼šæ‘„æ°åº¦è½¬åæ°åº¦ F = C * 2 + 30
  let celsius = 25
