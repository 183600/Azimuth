  // åŠ æ³•æ’ç­‰å¼ï¼ša + 0 = a
  let test_value = 42
  assert_eq(test_value, add(test_value, 0))
  
  // ä¹˜æ³•æ’ç­‰å¼ï¼ša * 1 = a
  assert_eq(test_value, multiply(test_value, 1))
  
  // ä¹˜æ³•é›¶æ€§è´¨ï¼ša * 0 = 0
