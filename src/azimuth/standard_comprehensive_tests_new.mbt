test "mathematical_distributive_property" {
  // æµ‹è¯•æ•°å­¦åˆ†é…å¾‹
  let a = 6
  let b = 4
  let c = 3
  
  // a * (b + c) = a * b + a * c
  let sum = add(b, c)
  let left_side = multiply(a, sum)
  
  let product1 = multiply(a, b)
  let product2 = multiply(a, c)
  let right_side = add(product1, product2)
  
  assert_eq(left_side, right_side)
}

