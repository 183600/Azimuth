test "complex_financial_calculations" {
  // å¤æ‚çš„é‡‘èè®¡ç®—åœºæ™¯æµ‹è¯•
  let principal = 10000
  let rate = 5
  let years = 3
  
  // è®¡ç®—å¤åˆ©ï¼ˆç®€åŒ–ç‰ˆæœ¬ï¼Œä½¿ç”¨å‘ä¸Šå–æ•´ï¼‰
  let year1 = @azimuth.add(principal, @azimuth.divide_with_ceil(@azimuth.multiply(principal, rate), 100))
