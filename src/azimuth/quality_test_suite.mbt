test "mathematical_distributive_property" {
  // æµ‹è¯•åˆ†é…å¾‹ï¼ša Ã— (b + c) = a Ã— b + a Ã— c
  let a = 3
  let b = 4
  let c = 5
  
  let left_side = multiply(a, add(b, c))
  let right_side = add(multiply(a, b), multiply(a, c))
  
  assert_eq(left_side, right_side)  // 3 Ã— (4 + 5) = 3 Ã— 4 + 3 Ã— 5 = 27
}

test "real_world_packaging_scenario" {
  // çœŸå®ä¸–ç•Œåœºæ™¯ï¼šåŒ…è£…è®¡ç®—
  let total_items = 47
  let items_per_box = 12
  
  let boxes_needed = divide_with_ceil(total_items, items_per_box)
  let actual_capacity = multiply(boxes_needed, items_per_box)
  let wasted_space = add(actual_capacity, -total_items)
  
  assert_eq(4, boxes_needed)      // ceil(47/12) = 4
  assert_eq(48, actual_capacity)  // 4 Ã— 12 = 48
  assert_eq(1, wasted_space)      // 48 - 47 = 1
}

