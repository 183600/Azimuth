  let baggage1 = baggage.with_entry("special.chars", "!@#$%^&*()")
  let baggage2 = baggage1.with_entry("unicode.value", "æµ‹è¯•ä¸­æ–‡ğŸš€")
  let baggage3 = baggage2.with_entry("url.encoded", "hello%20world")
