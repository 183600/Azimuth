  let monthly_income = 5000
  let rent = 1500
  let utilities = 200
  let food = 600
  let transportation = 300
  let entertainment = 200
  
  // è®¡ç®—æ€»æ”¯å‡º
  let total_expenses = add(add(add(add(rent, utilities), food), transportation), entertainment)
  
  // è®¡ç®—å‰©ä½™é¢„ç®—
