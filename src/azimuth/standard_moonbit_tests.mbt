  let base_value = 10
  let multiplier = 3
  let divisor = 4
  let addition = 5
  
  // è®¡ç®—æµç¨‹ï¼š((base * multiplier) / divisor) + addition
  let step1 = multiply(base_value, multiplier)
  let step2 = divide_with_ceil(step1, divisor)
  let result = add(step2, addition)
  
  assert_eq(13, result)  // ceil(10*3/4) + 5 = 8 + 5 = 13
}

