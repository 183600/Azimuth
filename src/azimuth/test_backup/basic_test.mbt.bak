test "test_1_basic_add_original" {
  if 5 != @azimuth.add(2, 3) {
    @builtin.panic()
  }
  if 0 != @azimuth.add(5, -5) {
    @builtin.panic()
  }
  if -10 != @azimuth.add(-7, -3) {
    @builtin.panic()
  }
}
test "test_1_basic_multiply_original" {
  if 6 != @azimuth.multiply(2, 3) {
    @builtin.panic()
  }
  if -6 != @azimuth.multiply(2, -3) {
    @builtin.panic()
  }
  if 21 != @azimuth.multiply(-7, -3) {
    @builtin.panic()
  }
}
test "test_1_basic_greet_original" {
  if "Hello, World!" != @azimuth.greet("World") {
    @builtin.panic()
  }
  if "Hello, !" != @azimuth.greet("") {
    @builtin.panic()
  }
  if "Hello, Azimuth!" != @azimuth.greet("Azimuth") {
    @builtin.panic()
  }
}
