test "mathematical_sequence_calculation" {
  // æµ‹è¯•æ•°å­¦åºåˆ—è®¡ç®—ï¼šç­‰å·®æ•°åˆ—æ±‚å’Œ
  let first_term = 3
  let common_diff = 4
  let num_terms = 6
  
  // æ‰‹åŠ¨è®¡ç®—æ•°åˆ—å’Œï¼š3 + 7 + 11 + 15 + 19 + 23
  let sum_manual = add(add(add(add(add(3, 7), 11), 15), 19), 23)
  
  // ä½¿ç”¨å…¬å¼è®¡ç®—ï¼šn/2 * (2a + (n-1)d)
  let sum_formula = divide_with_ceil(
    multiply(num_terms, add(multiply(2, first_term), multiply(num_terms - 1, common_diff))),
    2
  )
  
  assert_eq(78, sum_manual)
  assert_eq(78, sum_formula)
}

