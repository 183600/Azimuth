test "add_sequence_pattern" {
  // æµ‹è¯•åºåˆ—æ¨¡å¼ï¼šè¿ç»­åŠ æ³•
  let result = @azimuth.add(@azimuth.add(1, 2), @azimuth.add(3, 4))
