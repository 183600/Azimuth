///|
test "multiply_power_pattern" {
  // æµ‹è¯•å¹‚æ¨¡å¼ï¼šé‡å¤ä¹˜æ³•
  let base = 2
  let result = @azimuth.multiply(@azimuth.multiply(base, base), @azimuth.multiply(base, base))
  assert_eq(16, result)  // 2^4 = 16
  
  // è´Ÿæ•°å¹‚
  let base = -2
  let result = @azimuth.multiply(@azimuth.multiply(base, base), @azimuth.multiply(base, base))
  assert_eq(16, result)  // (-2)^4 = 16
  
  // é›¶å¹‚
  let base = 0
  let result = @azimuth.multiply(@azimuth.multiply(base, base), @azimuth.multiply(base, base))
  assert_eq(0, result)  // 0^4 = 0
}

